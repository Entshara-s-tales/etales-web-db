{
  "name": "etales-web-db",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format",
    "tswatch": "tsc --watch --noEmit --project './tsconfig.json'",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "prettier --check . && eslint .",
    "cry": "cry"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@storybook/addon-actions": "^6.2.0-rc.6",
    "@storybook/addon-essentials": "^6.2.0-rc.6",
    "@storybook/addon-links": "^6.2.0-rc.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/svelte": "^6.2.0-rc.9",
    "@sveltejs/adapter-netlify": "^1.0.0-next.0",
    "@sveltejs/adapter-node": "^1.0.0-next.0",
    "@sveltejs/kit": "^1.0.0-next.1",
    "@tailwindcss/jit": "^0.1.9",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "battlecry": "^0.3.6",
    "cosmiconfig": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.7",
    "postcss-load-config": "^3.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.35.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.4",
    "ts-jest": "^26.5.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.1.0"
  },
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,md}": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {},
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "<rootDir>/scripts/svelte-jester/transform.cjs",
        {
          "preprocess": true
        }
      ],
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "modulePathIgnorePatterns": [
      ".history"
    ]
  }
}
